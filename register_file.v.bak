/*_____________________________________
         32 32-bit GP Register File
  _____________________________________*/
  
  `timescale 1 ps / 100 fs
  
  module register_file (
	
		output reg [31:0] Read_Data1,
		output reg [31:0] Read_Data2,
		input		  [ 4:0] Read_Reg1,
		input 	  [ 4:0] Read_Reg2,
		input      [ 4:0] Write_Reg,
		input 	  [31:0] Write_Data,
		input					RegWrite
		
		);